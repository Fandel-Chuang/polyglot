// polyglot高级语义测试
>> "iostream"

@ Player {
    name: string,
    health: int,
    position: Vector3
}

& Player {
    Player(n: string, h: int) {
        _.name = n
        _.health = h
    }

    getHealth() -> int {
        <- _.health
    }
}

main() {
    player := Player("Alice", 100)
    hp := player.getHealth()
    badVar := undeclaredFunction()  // 错误：未声明的函数

    <- 0
}